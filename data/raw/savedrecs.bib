@article{ WOS:001579113200001,
Author = {Karlsen, Solveig and Kersting, Magdalena and Odegaard, Marianne and
   Olufsen, Magne and Suhr, Mai Lill and Kjaernsli, Marit},
Title = {A comparative study of teachers' conceptualisations and enactment of
   inquiry-based science education},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE EDUCATION},
Year = {2025},
Month = {2025 SEP 25},
Abstract = {Inquiry-based science education (IBSE) is widely championed as an
   effective instructional approach, with teachers being key agents in its
   implementation. While substantial research exists on students' learning
   outcomes in IBSE, the relationship between teachers' conceptions of IBSE
   and their classroom implementation remains unclear. This study addresses
   this gap by comparing teachers' expressed understandings and enactments
   of inquiry practices through semi-structured interviews and classroom
   observations of six science teachers (three primary and three
   lower-secondary). Combining thematic analysis of interview data with
   video analysis of 24 lessons (four for each teacher) using an
   observation manual, we find varied conceptions of IBSE among the
   teachers that only partially align with their classroom practices. We
   identify two alignment challenges: student autonomy versus
   teacher-provided structure and implicit expectations versus explicit
   instructional strategies. These challenges constrain effective IBSE
   implementation and highlight challenges teachers face in aligning
   theoretical ideals with classroom realities. We argue that understanding
   these challenges is crucial for improving teacher training and
   professional development to realise the full potential of IBSE.},
