@article{ WOS:001060200900001,
Author = {Mang, Ha My Anna and Chu, Hye-Eun and Martin, Sonya N. and Kim,
   Chan-Jong},
Title = {Developing an Evaluation Rubric for Planning and Assessing SSI-Based
   STEAM Programs in Science Classrooms},
Journal = {RESEARCH IN SCIENCE EDUCATION},
