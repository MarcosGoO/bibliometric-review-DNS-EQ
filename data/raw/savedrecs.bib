Title = {Repurposing an Introductory Organic and Inorganic Laboratory Course from
   the Focus on Teaching Theory to the Focus on Teaching Practical
   Technique},
Journal = {JOURNAL OF CHEMICAL EDUCATION},
Year = {2021},
Volume = {98},
