@article{ WOS:001623249400001,
Author = {Ribau, Isabel},
Title = {The implementation of practical work in chemistry, along with the
   principles of green chemistry and sustainable chemistry, in Portugal},
Journal = {CHEMISTRY TEACHER INTERNATIONAL},
Year = {2025},
Volume = {7},
Number = {4},
Pages = {657-680},
Month = {DEC 29},
Abstract = {Since the late twentieth century, global sustainability and greener
   chemistry have become essential human goals. Education is crucial in
   driving this change by enhancing scientific knowledge and literacy.
   Since the early 2000s, the Portuguese chemistry curriculum for middle
   and high school has included mandatory laboratory activities to increase
   student enrollment in science courses, but also scientific skills,
   learning outcomes, procedural knowledge, and understanding in chemistry
   while fostering communication, attitudes, and reasoning abilities
   essential for scientific literacy. An IUPAC survey, aimed to enhance
   understanding of practical work in chemistry, focusing on the
   application of green and sustainable chemistry principles, was applied
   among Portuguese chemistry teachers. Data were collected through online
   dissemination nationwide. Results indicate that most participants
   incorporate laboratory activities into their teaching, following the
   chemistry textbook and curriculum guidelines, and conduct green
   chemistry activities. Teachers believe that curricula for middle and
   high school, as well as teacher training in higher education, should
   include more practical work and focus on sustainability and green
