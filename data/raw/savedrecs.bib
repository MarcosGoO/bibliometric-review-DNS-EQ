@article{ WOS:000857466500001,
Author = {Zhang, Jinglu and Browne, William J.},
Title = {Exploring Chinese high school students' performance and perceptions of
   scientific argumentation by understanding it as a three-component
   progression of competencies},
Journal = {JOURNAL OF RESEARCH IN SCIENCE TEACHING},
