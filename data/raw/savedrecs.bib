@article{ WOS:001521373600008,
Author = {Chang, Jina and Park, Joonhyeong and Na, Jiyeon and Hong, Oksu},
Title = {Development and Validation of an Instructional Model for Risk Response
   in Science Education},
Journal = {ASIA-PACIFIC SCIENCE EDUCATION},
Year = {2025},
Volume = {11},
Number = {1},
Pages = {229-260},
Month = {JUN},
Abstract = {This study was aimed at developing and validating an instructional model
   to foster K-12 students' risk-response competencies. Following a
   literature review and assessment from experts, a preliminary model was
   refined into an instructional model of risk-response science education.
   The model comprised three instructional stages: (1) scientific awareness
   of risk, (2) scientific assessment of risk, and (3) scientific actions
   for risk management. The first stage involves students understanding
   risk's scientific and sociocultural aspects. The second stage involves
   analyzing data or predicting risk's impacts through analytic and
   scientific inquiry. The third stage involves the development and
   application of management strategies to address risks. In addition, to
   support teachers in implementing risk-response science activities, four
   instructional design principles are proposed: fostering students'
   understanding of the nature of risk, promoting critical reasoning
   through data analysis, providing opportunities for action and
   participation in risk response, and gradually expanding activities'
   scope. Finally, educational implications are discussed.},
