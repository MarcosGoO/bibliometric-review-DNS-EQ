@article{ WOS:001483418900001,
Author = {Jia, Shuqi and Ren, Shanzhang},
Title = {The Significance of Science Content Knowledge for Socioscientific
   Reasoning Competency: Evidence from SSI Instruction on Environmental
   Topics Based on the SIMBL Model},
