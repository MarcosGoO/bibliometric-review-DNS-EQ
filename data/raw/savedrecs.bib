@article{ WOS:001523607300001,
Author = {Fu, Xinjian and Zhan, Qinglong},
Title = {Effectiveness of argument in science education: a meta-analysis based on
   the results of 17 experiments},
Journal = {RESEARCH IN SCIENCE \& TECHNOLOGICAL EDUCATION},
Year = {2025},
Month = {2025 JUL 7},
Abstract = {BackgroundIn science education, the use of argumentation as a teaching
   method is a valuable approach that helps students understand scientific
   concepts, develop scientific thinking, and enhance critical thinking
   skills. The use of argumentative teaching in science education has
   become more popular, but the evidence for its effectiveness is still
   scarce.PurposeThis study aims to explore the effectiveness of
   argumentation in science education on students' academic performance
   through a meta-analysis. Specifically, it examines its impact on
   students' academic achievement, scientific process skills, conceptual
   knowledge, and attitude.SampleThe study sample was extracted from 13
   academic papers retrieved from professional scholarly databases (Web of
   Science, Scopus, and EBSCO) using a systematic search strategy. A total
   of 17 experimental datasets were obtained, involving 879
   participants.Design and methodsThis study employs meta-analysis to
   calculate the effect sizes of the 17 datasets. Additionally, moderator
   analyses were conducted to identify the sources of
   heterogeneity.ResultsRandom effects model results showed a significant
   overall positive effect of argumentation on student learning outcomes
   (Effect Size = 0.704, 95\% confidence interval {[}0.516-0.893]). We
   found that teaching with the introduction of argumentation can help
