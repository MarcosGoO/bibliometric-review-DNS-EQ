Author = {Tsai, Chun-Yen and Huang, Tai-Chu},
Title = {The Relationship Between Adult Self-Efficacy and Scientific
   Competencies: the Moderating Effect of Gender},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
