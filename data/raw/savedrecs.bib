@article{ WOS:001617454600001,
Author = {Cai, Su and Huang, Hainachuan and Jiao, Xinyue and Zhang, Yuhua},
Title = {Generative AI plus AR: a novel embodied conversational agent in an
   interactive learning environment},
Journal = {INTERACTIVE LEARNING ENVIRONMENTS},
Year = {2025},
Month = {2025 NOV 19},
Abstract = {Generative Artificial Intelligence (Generative AI) and Augmented Reality
   (AR) have garnered increasing attention from researchers for their
   application in the field of education. Investigating how to integrate
   Generative AI in a visual way into real classrooms is one of the
   directions for the deep integration of information technology and
   science education in the future. This study designed and developed an
   AR-based embodied conversational agent (ECA) Virtual Scientist and
   explored its impact on students' learning outcomes and experience. A
   total of 62 primary school students participated in a four-week
   experiment, in which the quantitative and qualitative data were
   collected to be analyzed through covariance analysis and thematic
   analysis. It is found that the intervention of the AR-based embodied
   conversational agent significantly enhanced students' critical thinking,
   inquisitiveness and learning motivation. Moreover, students positively
   evaluated the technology. This study provides experience and strategies
   for the development and practical classroom application of Generative
   AI-based AR learning environment, offering practical guidance for the
   implementation of human-computer teaching.},
